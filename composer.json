{
    "name": "eriknyk/phpalchemy",
    "description": "The PHP framework",
    "keywords": ["php framework"],
    "homepage": "http://phpalchemy.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Erik Amaru Ortiz",
            "email": "aortiz.erik@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        },
        {
            "type":         "pear",
            "url":          "pear.php.net",
            "vendor-alias": "pear"
        }
    ],
    "require": {
        "crodas/Haanga"   : "dev-master",
        "crodas/Notoj"    : "dev-master",
        "symfony/console" : "2.0.15",
        "zendframework/zend-filter": "2.0.*",
        "pear-pear.php.net/pear":               "*",
        "pear/archive_tar": "1.3.*"
    },
    "autoload": {
        "psr-0": { "Alchemy": "./" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
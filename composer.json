{
    "name": "phpalchemy/phpalchemy",
    "description": "The PHP framework",
    "keywords": ["php framework"],
    "homepage": "http://phpalchemy.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Erik Amaru Ortiz",
            "email": "aortiz.erik@gmail.com"
        }
    ],
    "repositories": [{
        "type":"package",
        "package": {
          "name": "haanga/haanga",
          "version":"1.0.5",
          "source": {
              "url": "https://github.com/crodas/Haanga.git",
              "type": "git",
              "reference":"master"
            }
        }
    }],
    "require": {
        "haanga/haanga"   : "1.0.4",
        "crodas/Notoj"    : "0.1.13",
        "symfony/console" : "2.2.*"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": { "Alchemy": "Alchemy/" }
    }
}

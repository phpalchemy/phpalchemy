<?xml version="1.0" encoding="UTF-8"?>
<extjs> <!--https://developer.mozilla.org/en/XUL_Reference-->
  <!-- <global>
    <attribute mapping="type" name="xtype"/>
  </global> -->
  <widget mapping="textbox" name="textfield">
    <attribute mapping="id" name="id"/>
    <attribute mapping="name" name="name"/>
    <attribute mapping="fieldLabel" name="fieldLabel"/>
    <attribute mapping="size" name="size"/>
    <attribute mapping="type" name="xtype"/>
  </widget>

  <widget mapping="textarea" name="textarea">
    <attribute mapping="label" name="id"/>
    <attribute mapping="name" name="name"/>
    <attribute mapping="fieldLabel" name="fieldLabel"/>
    <attribute mapping="size" name="size"/>
    <attribute mapping="rows" name="rows"/>
    <attribute mapping="type" name="xtype"/>
  </widget>

  <widget name="checkbox" mapping="checkbox">
    <attribute mapping="id" name="id"/>
    <attribute mapping="name" name="name"/>
    <attribute mapping="fieldLabel" name="fieldLabel"/>
    <attribute mapping="label" name="boxLabel"/>
    <attribute mapping="checked" name="checked"/>
    <attribute mapping="type" name="xtype"/>
  </widget>
</extjs>